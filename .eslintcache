p>no comments yet </p>\n            return <>no comments </>\n        } else if(comments ) {\n            return comments.map(comment => <ArticleComment key={comment.id} commentObj={comment} />)\n        } else {\n            return <>no comments </>\n        }\n\n       \n    }\n\n    renderCommentButton = () => {\n        // console.log(\"rendering comment button this.props.userObj\",this.props.userObj)\n        \n        console.log(this.props.userObj)\n        if(this.props.userObj){\n            return <button >Button</button>\n        }\n    }\n\n    commentButtonHandler = () => {\n        // if(this.state.commentFormClicked ){\n        //     this.setState({commentFormClicked: false})\n        //     console.log(this.state.commentFormClicked)\n        // } else {\n        //     this.setState({commentFormClicked: true})\n        //     console.log(this.state.commentFormClicked)\n            \n        // }\n        this.setState({commentFormClicked: !this.state.commentFormClicked })\n\n    }\n\n    renderCommentForm = () => {\n        if(this.state.commentFormClicked && this.props.userObj){\n\n            return (<AddComment fetchComments={this.fetchComments} />)\n        } \n\n    }\n\n    showCommentButton = ()=> {\n        // if(this.state.commentFormClicked && this.props.userObj){\n        //     return <button onClick={this.commentButtonHandler}>Nevermind</button>\n        // } else if (this.state.commentFormClicked === false){\n        //     return <button onClick={this.commentButtonHandler}>Comment</button>\n        // } else {\n        //     return <button onClick={this.commentButtonHandler}>Comment</button>\n\n        // }\n        if(this.props.userObj && this.state.commentFormClicked){\n            return <button onClick={this.commentButtonHandler}>Nevermind</button>\n        } else if (this.props.userObj && !this.state.commentFormClicked ){    \n            return <button onClick={this.commentButtonHandler}>Comment</button>\n        } else {\n            return\n        }\n\n    }\n    \n    render() {\n        return (\n            <>\n\n                <h3>comments:</h3>\n                \n                {this.renderComments()} \n                <br></br>\n                {/* {this.renderCommentButton()} */}\n                {this.renderCommentForm()}\n\n                {/* {this.state.commentFormClicked && this.props.userObj? \n                <>\n                <button onClick={this.commentButtonHandler}>Nevermind</button>\n                </>\n                :\n                <>\n\n                <button onClick={this.commentButtonHandler}>Comment</button>\n\n                </>\n                } */}\n\n                {this.showCommentButton()}\n                \n\n            </>\n        )\n    }\n}\nconst msp = (state) => {\n    return {\n    \n        userObj: state.user,\n        currentArticle: state.article\n    }\n}\n\nexport default connect(msp,null)(ArticleCommentsContainer)","/Users/stevenwu/Flatiron/final-project/ganktank-frontend/src/Components/ArticleComment.js",[],"/Users/stevenwu/Flatiron/final-project/ganktank-frontend/src/Containers/ArticlesContainer.js",["61","62"],"/Users/stevenwu/Flatiron/final-project/ganktank-frontend/src/Redux/rootReducer.js",[],"/Users/stevenwu/Flatiron/final-project/ganktank-frontend/src/Components/Signup.js",[],"/Users/stevenwu/Flatiron/final-project/ganktank-frontend/src/Components/Navbar.js",[],"/Users/stevenwu/Flatiron/final-project/ganktank-frontend/src/Components/ArticleCard.js",["63"],"/Users/stevenwu/Flatiron/final-project/ganktank-frontend/src/App.js",["64"],"/Users/stevenwu/Flatiron/final-project/ganktank-frontend/src/Components/AddArticle.js",[],"/Users/stevenwu/Flatiron/final-project/ganktank-frontend/src/Components/ArticleRatingForm.js",[],"/Users/stevenwu/Flatiron/final-project/ganktank-frontend/src/Components/ShowArticle.js",[],{"ruleId":"65","severity":1,"message":"66","line":4,"column":9,"nodeType":"67","messageId":"68","endLine":4,"end